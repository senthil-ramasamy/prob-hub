<p id="notice"><%= notice %></p>

<h1>Listing Solns</h1>

<table>
  <thead>
    <tr>
      <th>Solnname</th>
      <th>Solntext</th>
      <th>Solnauthor</th>
      <th>Solnid</th>
      <th>Solndate</th>
      <th>Prob</th>
      <th>Solnvotes</th>
      <th>Solnrating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solns.each do |soln| %>
      <tr>
        <td><%= soln.solnname %></td>
        <td><%= soln.solntext %></td>
        <td><%= soln.solnauthor %></td>
        <td><%= soln.solnid %></td>
        <td><%= soln.solndate %></td>
        <td><%= soln.prob %></td>
        <td><%= soln.solnvotes %></td>
        <td><%= soln.solnrating %></td>
        <td><%= link_to 'Show', soln %></td>
        <td><%= link_to 'Edit', edit_soln_path(soln) %></td>
        <td><%= link_to 'Destroy', soln, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Soln', new_soln_path %>
