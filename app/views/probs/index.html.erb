<p id="notice"><%= notice %></p>

<h1>Listing Probs</h1>
<div class="container qw">
    

    <table class="table">
        <thead>
          <tr>
            <th>#</th>
            <th>Title</th>
            <th>Status</th>
            <th>Votes</th>
          </tr>
        </thead>
        <tbody>
          <% @probs.each do |prob| %>
            <tr>
              <td><h4><%= prob.pid %><h4></td>
              <td>
                <h4 class="head"><%= prob.pname %></h4>
                <ul class="post-meta">
                  <li><i class="fa fa-pencil-square-o"></i><strong> Posted By:</strong> <%= prob.person %></li>
                  <li><i class="fa fa-clock-o"></i><strong> Posted On:</strong> <%= prob.pdate %></li>
                  <li><i class="glyphicon glyphicon-star"></i><strong> Ratings: <%= prob.rateing %></strong></li>
                  <li><i class="glyphicon glyphicon-comment"></i><strong> Solutions:</strong></li>
                </ul>
                <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat.</p>
                
              </td>
              <td><h4><%= prob.rateing %><h4></td>
              <td><h4><%= prob.votes %></h4></td>
            </tr>
          </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prob', new_prob_path %>
