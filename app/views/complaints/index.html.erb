<p id="notice"><%= notice %></p>

<h1>Listing Complaints</h1>

<table>
  <thead>
    <tr>
      <th>Cid</th>
      <th>Ctext</th>
      <th>Cfield</th>
      <th>Cactionseq</th>
      <th>Ifprivate</th>
      <th>Cpersonal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @complaints.each do |complaint| %>
      <tr>
        <td><%= complaint.cid %></td>
        <td><%= complaint.ctext %></td>
        <td><%= complaint.cfield %></td>
        <td><%= complaint.cactionseq %></td>
        <td><%= complaint.ifprivate %></td>
        <td><%= complaint.cpersonal %></td>
        <td><%= link_to 'Show', complaint %></td>
        <td><%= link_to 'Edit', edit_complaint_path(complaint) %></td>
        <td><%= link_to 'Destroy', complaint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Complaint', new_complaint_path %>
