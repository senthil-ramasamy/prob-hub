<%= simple_form_for(@complaint) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :cid %>
    <%= f.input :ctext %>
    <%= f.input :cfield %>
    <%= f.input :cactionseq %>
    <%= f.input :ifprivate %>
    <%= f.input :cpersonal %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
