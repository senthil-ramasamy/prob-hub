<%= simple_form_for(@challenge) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :chaltext %>
    <%= f.input :chalname %>
    <%= f.input :chaldate %>
    <%= f.input :chalprize %>
    <%= f.input :chalsponsor %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
